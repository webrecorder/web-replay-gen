---
layout: layout.njk
---

<section class="max-w-screen-lg mx-auto">
  <h1 class="my-4 text-xl text-center text-neutral-600">Browse web archives</h1>

  <main class="p-2">
    <ul class="border rounded">
      {% for page in archivePages %}
      <li class="border-t first:border-t-0">
        <a
          href="{{ page.url }}"
          class="block p-2 text-neutral-700 hover:bg-neutral-50 hover:text-neutral-900"
          >{{ page.name }}</a
        >
      </li>
      {% endfor %}
    </ul>
  </main>
</section>
